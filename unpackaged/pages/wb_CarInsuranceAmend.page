                                 <apex:actionSupport event="onchange" action="{!row.fetchServiceCommission}"
                                                    reRender="ServiceCommission__c"/> 
                            </apex:inputfield>
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
                                    <div style="width:100px;">
                                        <apex:outputText value="{!$ObjectType.wb_ReceivableServiceFee__c.fields.ServiceCommission__c.label}"></apex:outputText><br/>
                                    </div>
                                </apex:outputPanel>
                            </apex:facet>
