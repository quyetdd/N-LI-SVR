                                        action="{!ReplacementDateValidation}" reRender="theForm" />
                    
                </apex:pageBlockSection>
                
                <apex:outputText rendered="{!InvalidReplacementDate}" id="InvalidReplacementDate">
                    <script>
                    
                    try {
                        alert('{!InvalidReplacementDateMessage}');
                        CallApexMethod2ResetReplacementDateValidationStatus();
                        
                    }catch(e)
                    {
                        alert(e);
                    }      
                    //self.close();
                    </script>
                </apex:outputText>
                
                <apex:pageBlockSection id="theAddtionPassengerSection"  rendered="{!IF((CarInsuranceAdditionalCar.Car__c!=null), true , false)}">
                    
                    <apex:commandButton action="{!NewAdditionalPassengerInsurance}" value="追加" /> 
                    
                    <div>
                        <apex:variable var="index" value="{!0}" /> 
                    </div>
                    
                    
                    <script>
                    function confirmation(id) {  
                        if (!confirm('Do you want to delete row:' +id +'?')) return false;
                        else return true;
                    }
                    function openPopupConfirmDeletePassenger(idx){
                        try {
                            if (confirmation(idx)) {
                                //alert(id);
                                CallApexMethod2DeletePassenger(idx);
                            }
                            
                        }catch(e)
                        {
                            alert(e);
                        }                       
                        return false;
                    }
                    
                    </script>
                    
