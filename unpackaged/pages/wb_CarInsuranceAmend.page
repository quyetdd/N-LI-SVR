                        </apex:inputCheckbox>
                    </apex:column>
                    
                    <apex:column rendered="{!IF((ChangeOption.OpnCarExchange==true), true , false)}">
                        <apex:facet name="header">
                            <apex:outputPanel >
                                <div style="width:150px;">
                                    <apex:outputText value="異動開始日0時"></apex:outputText><br/>
                                </div>
                            </apex:outputPanel>
                        </apex:facet>
                        <apex:inputfield id="ReplacedDate__c" value="{!o.NewCar.ReplacedDate__c}" style="width:100px;"/>
                    </apex:column>
                    
                    
                </apex:dataTable>
                
                <apex:commandButton value="Apply" action="{!ApplyChange}" reRender="theForm" rendered="{!IF((ChangeOption.OpnCarTerminate==true), true , false)}"/> 
                <!--
<div style="width:900px;">
<apex:outputText value="{!theMessage}"></apex:outputText><br/>
</div>
-->
