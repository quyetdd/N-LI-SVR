                        <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
                                    <div style="width:100px;">
                                        <apex:outputText value="{!$ObjectType.wb_ReceivableServiceFee__c.fields.OptionName__c.label}"></apex:outputText><br/>
                                    </div>
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:outputfield value="{!row.Refund.OptionName__c}" style="width:100px;"></apex:outputfield>
                        </apex:column>
                        
                        <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
                                    <div style="width:100px;">
                                        <apex:outputText value="{!$ObjectType.wb_ReceivableServiceFee__c.fields.OptionAmount__c.label}"></apex:outputText><br/>
                                    </div>
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:inputfield value="{!row.Refund.OptionAmount__c}" style="width:100px;"  rendered="{!IF((row.Refund.OptionName__c!=null), true , false)}"></apex:inputfield>
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
                                    <div style="width:100px;">
                                        <apex:outputText value="{!$ObjectType.wb_ReceivableServiceFee__c.fields.OptionCommission__c.label}"></apex:outputText><br/>
                                    </div>
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:inputfield id="OptionCommission__c" value="{!row.Refund.OptionCommission__c}" style="width:100px;"  rendered="{!IF((row.Refund.OptionName__c!=null), true , false)}"></apex:inputfield>
                        </apex:column>
                        <!--
                         <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
                                    <div style="width:700px;">
                                        <apex:outputText value="{!$ObjectType.wb_ReceivableServiceFee__c.fields.Memo__c.label}"></apex:outputText><br/>
                                    </div>
                                </apex:outputPanel>
                            </apex:facet>
                            <apex:inputtext value="{!row.Memo}" style="font-size:9pt;font-weight:bold!important;width:700px;"></apex:inputtext>
                        </apex:column>
                        <apex:column >
                            <apex:facet name="header">
                                <apex:outputPanel >
