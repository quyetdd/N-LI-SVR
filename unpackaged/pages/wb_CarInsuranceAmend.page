                                </div>
                            </apex:outputPanel>
                        </apex:facet>
                        <apex:inputcheckbox id="CpsTerminate" value="{!o.CpsTerminate}"/>
                    </apex:column>                        
                    
                    <apex:column rendered="{!IF((ChangeOption.OpnCarTerminate==true&&ChangeOption.ValidVlt==true), true , false)}"  >
