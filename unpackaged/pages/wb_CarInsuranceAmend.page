                                <div style="width:100px;">
                                    <apex:outputText value="入れ替え"></apex:outputText><br/>
                                </div>
                            </apex:outputPanel>
                        </apex:facet>
                        <apex:inputCheckbox value="{!o.OpnCarExchange}" style="width:100px;">
                            <apex:actionSupport event="onchange" action="{!o.fetchOpnCarExchange}"
