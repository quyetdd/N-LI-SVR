                    </apex:dataTable>
                    <br/> 
                    <apex:commandButton value="Confirm and Save Refund"
                                        onclick="if(!confirm('Are you sure?')){return false};"
                                        action="{!SaveRefund}" reRender="replacementPsgr,theForm" /> 
                    
                </apex:pageBlockSection>   
                
                
            </apex:pageBlockSection>
            
            <apex:pageBlockSection columns="1" id="theAddtionSection" rendered="{!IF((CIC_ID=='null'||ChangeOption.OpnCarExchange==true), true , false)}"
                                   title="{!IF((CIC_ID=='null'), '増車詳細' , '入れ替え車')}">  
                
                <apex:pageBlockSection columns="1" id="theAddtionCarSection"  >
                    
                    <apex:dataTable value="{!CarInsuranceAdditionalCar}" var="row"  cellpadding="4" border="1" onRowClick="highlight(this);">
                        
