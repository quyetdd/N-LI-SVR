            <script>
            
            
            
            function ExportData(Id, Type){
                //  alert(Type);
                
                try {
                    var baseURL = getBaseURL();
                    if (Type == '請求書') 
                        baseURL = baseURL + "apex/wb_Membership_InvoiceExport";
                    else {
                        if (Type == '継続申込書') 
                            baseURL = baseURL + "apex/wb_Membership_RenewalExport";
                        else if (Type == '見積書') 
                            baseURL = baseURL + "apex/wb_Membership_QuotationExport";
                         }
                    
                    var para ='';
                    para　='?Id=' +Id  ;
                    para = para + '&pv0=YES' ; 
                    window.open(baseURL + para );
                    
                }
                catch(e)
                {
                    alert(e);
                } 
            }
            
            function ExportTemplate(Type){
                
                
                try {
                    var baseURL = getBaseURL();
                    baseURL = baseURL + "/servlet/servlet.FileDownload";
                    
                    //var para='?file=0155D000000Fa1h';
                    
                    //var para='?file=01510000008cfPB'; //Production
                    var para;
                    if (Type == '請求書') {
                        para='?file=01510000008cfPB'; //Production
                    }
                    else {
                        if (Type == '継続申込書') 
                            para='?file=01510000008co05'; //Production
                        else if (Type == '見積書') 
                            para='?file=01510000008drrv'; //Production
                    }
                    
                    window.open(baseURL + para );
                    
                }
                catch(e)
                {
                    alert(e);
                } 
            }
            
